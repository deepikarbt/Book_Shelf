<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.deepika.rbtlib">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:icon="@drawable/appicon">
        <activity
            android:name="com.example.deepika.rbtlib.Splashscreen"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.deepika.rbtlib.Login"
            android:theme="@style/Theme.AppCompat.NoActionBar"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.deepika.rbtlib.MainActivity"
            android:screenOrientation="portrait"
            android:label="Home"
            android:theme="@style/Base.Theme.AppCompat.Light.DarkActionBar"
            />
        <activity
            android:name="com.example.deepika.rbtlib.UploadActivity"
            android:label="Checkout"
            android:theme="@style/Base.Theme.AppCompat.Light.DarkActionBar"
            android:screenOrientation="portrait" >
        </activity>
       <activity android:name=".Receive"
           android:label="Checkin"
           android:theme="@style/Base.Theme.AppCompat.Light.DarkActionBar"  />
        <activity android:name=".Config"/>
    </application>
</manifest>
